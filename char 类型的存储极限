#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <string.h>
int main() {
	char a[1000];
	for (int i = 0; i < 1000; i++) {
		a[i] = -1 - i;
	}
	printf("%d", strlen(a));

}
//-1 -> -128 -> 127 -> 126 -> 0
// 
// 
// 
//10000000 00000000 00000000 00001010
//11111111 11111111 11111111 11110101
//11111111 11111111 11111111 11110110

// 1111 1111
// 0000 0000
// 1111 1111
// 1000 0001 -1

// -1 + -127
// 1111 1111 -1
// 1111 1111 -127
// 1000 0000
// 1000 0001 -127
//11000 0000 -128
// 0000 0000 原码-128

// 0000 0000
// 0111 1111
// 1000 0000


//本身已经越界了，所以变成正数很正常
// -1 + -128
// 1111 1111 -1
// 1000 0000 -128
//10111 1111 127

// 1 1000 0001 -129 原码
// 1 0111 1110  反码
// 1 0111 1111  补码

// 1111 1111 -1
// 0111 1111 -129
//10111 1110 126

int main() {
	int i = 0;
	while (i < 10) {
		printf("continue befor : %d\n", i);
		if (i == 5) {
			printf("%d\n", i);
			continue;
		}
		printf("continue after : %d\n", i);
		i++;
	}
}
一运行就知道
